{
  "name": "nazalas/laravel-google-cloud-storage",
  "description": "A Google Cloud Storage filesystem for Laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "Superbalist.com a division of Takealot Online (Pty) Ltd",
      "email": "info@superbalist.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "illuminate/support": "5.4.*||5.5.*||5.6.*||5.7.*||5.8.*||^6.0||^7.0||^8.0||^9.0",
    "nazalas/flysystem-google-storage": "^9.0",
    "illuminate/filesystem": "5.4.*||5.5.*||5.6.*||5.7.*||5.8.*||^6.0||^7.0||^8.0||^9.0",
    "nazalas/flysystem-cached-adapter": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Superbalist\\LaravelGoogleCloudStorage\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "Superbalist\\LaravelGoogleCloudStorage\\GoogleCloudStorageServiceProvider"
      ]
    }
  },
  "require-dev": {
    "phpunit/phpunit": ">=4.8"
  }
}
